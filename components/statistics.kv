#:kivy 2.3.0

<Statistics>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_x: 1
        size_hint_y: None

        MDLabel:
            halign: 'center'
            text: root.title
            font_size: '34sp'
            theme_text_color: "Custom"
            text_color: root.primary_color